<Window x:Class="sabirov.telbot.db.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sabirov.telbot.db"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Margin="10" FontSize="25px" x:Name="startBTN" Click="startBTN_Click">Start</Button>
            <Button Margin="10" FontSize="25px" x:Name="stopBTN" Click="stopBTN_Click">Stop</Button>
            <Label x:Name="isreceiveLB"></Label>
        </StackPanel>
    </Grid>
</Window>
